<questao>
    <title>Questão ordenamento</title>
    <enunciado>Crie um algoritmo que recebe um array desordenado, e faça o ordenamento do mesmo</enunciado>
    <inputExample>[5, 3, 1, 4, 2]</inputExample>
    <outputExample>1, 2, 3, 4, 5</outputExample>
    <baseCode>
        <language>php</language>
        <code><![CDATA[
        <?php
        $lista = explode(",", $argv[1]);
        ?>]]></code>
    </baseCode>
    <correctCode>
        <language>php</language>
        <code><![CDATA[<?php $lista = explode(",", $argv[1]);sort($lista);echo implode(",", $lista);?>]]></code>
    </correctCode>
    <testCases>
        <testCase>
            <input>1, 2, 3</input>
            <expectedOutput>1, 2, 3</expectedOutput>
        </testCase>
        <testCase>
            <input>4, 5, 6</input>
            <expectedOutput>4, 5, 6</expectedOutput>
        </testCase>
        <testCase>
            <input>7, 2, 5, 3, 9</input>
            <expectedOutput>2, 3, 5, 7, 9</expectedOutput>
        </testCase>
        <testCase>
            <input>10, 1, 4, 7, 3, 8</input>
            <expectedOutput>1, 3, 4, 7, 8, 10</expectedOutput>
        </testCase>
        <testCase>
            <input>20, 15, 5</input>
            <expectedOutput>5, 15, 20</expectedOutput>
        </testCase>
        <testCase>
            <input>8, 4, 3, 2, 1</input>
            <expectedOutput>1, 2, 3, 4, 8</expectedOutput>
        </testCase>
        <testCase>
            <input>12, 15, 10, 3</input>
            <expectedOutput>3, 10, 12, 15</expectedOutput>
        </testCase>
        <testCase>
            <input>25, 2, 5, 20, 3</input>
            <expectedOutput>2, 3, 5, 20, 25</expectedOutput>
        </testCase>
        <testCase>
            <input>0, -1, 2, -3, 5</input>
            <expectedOutput>-3, -1, 0, 2, 5</expectedOutput>
        </testCase>
        <testCase>
            <input>9, 3, 7, 5, 8</input>
            <expectedOutput>3, 5, 7, 8, 9</expectedOutput>
        </testCase>
    </testCases>
</questao>
